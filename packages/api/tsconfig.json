{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es6"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "rootDir": "src",
"outDir": "build"
  },
  "exclude": ["node_modules"],
  "include": ["**/*.ts", "./src/server.ts"]
}
